<?xml version="1.0" encoding="UTF-8"?>
<package packagerversion="1.4.6" version="2.0"
  xmlns="http://pear.php.net/dtd/package-2.0"
  xmlns:tasks="http://pear.php.net/dtd/tasks-1.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://pear.php.net/dtd/tasks-1.0 http://pear.php.net/dtd/tasks-1.0.xsd http://pear.php.net/dtd/package-2.0 http://pear.php.net/dtd/package-2.0.xsd">
  <name>sfAjaxDebugPlugin</name>
  <channel>plugins.symfony-project.org</channel>
  <summary>web debug toolbar for AJAX applications</summary>
  <description>
    This plugin provides a way to make the web debug toolbar useful again
    for AJAX applications.
  </description>
  <lead>
    <name>Andreas Ferber</name>
    <user>aferner</user>
    <email>af+symfony@chaos-agency.de</email>
    <active>yes</active>
  </lead>
  <date>2008-07-08</date>
  <version>
    <release>0.1.0</release>
    <api>0.1.0</api>
  </version>
  <stability>
    <release>beta</release>
    <api>beta</api>
  </stability>
  <license uri="http://www.symfony-project.com/license">MIT license</license>
  <notes>-</notes>
  <contents>
    <dir name="/">
      <file role="data" name="README" />
      <file role="data" name="LICENSE" />
      <dir name="config">
        <file role="data" name="config.php" />
        <file role="data" name="factories.yml" />
        <file role="data" name="settings.yml" />
      </dir>
      <dir name="lib">
        <file role="data" name="sfAjaxDebugRouting.class.php" />
        <file role="data" name="sfAjaxDebugLogger.class.php" />
      </dir>
      <dir name="modules">
        <dir name="sfAjaxDebug">
          <dir name="actions">
            <file role="data" name="actions.class.php" />
          </dir>
          <dir name="config">
            <file role="data" name="security.yml" />
            <file role="data" name="view.yml" />
          </dir>
          <dir name="templates">
            <file role="data" name="cssSuccess.css.php" />
            <file role="data" name="jsSuccess.js.php" />
          </dir>
        </dir>
      </dir>
    </dir>
  </contents>
  <dependencies>
    <required>
      <php>
        <min>5.2.0</min>
      </php>
      <pearinstaller>
        <min>1.4.1</min>
      </pearinstaller>
      <package>
        <name>symfony</name>
        <channel>pear.symfony-project.com</channel>
        <min>1.1.0</min>
        <max>1.2.0</max>
        <exclude>1.2.0</exclude>
      </package>
    </required>
  </dependencies>
  <phprelease />
  <changelog />
</package>
